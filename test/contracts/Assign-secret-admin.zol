pragma solidity ^0.8.0;

contract MyContract {

    secret uint256 private a;
    secret address private admin;

    function setAdmin(secret address newAdmin) public {
      admin = newAdmin;
    }


    function assign(secret uint256 param1) public {
        require(msg.sender == admin);
        a = param1;
    }
}
